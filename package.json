{
    "name": "liora",
    "version": "7.6.5",
    "description": "Modern, high-performance WhatsApp bot with Rust supervisor, Bun runtime, and advanced caching architecture",
    "keywords": [
        "whatsapp",
        "whatsapp-bot",
        "baileys",
        "bun",
        "rust",
        "automation",
        "bot-framework",
        "websocket",
        "redis",
        "postgresql",
        "async",
        "tokio",
        "performance"
    ],
    "type": "module",
    "private": false,
    "engines": {
        "bun": ">=1.1.0",
        "node": ">=24.0.0",
        "npm": "please-use-bun"
    },
    "os": [
        "linux",
        "darwin"
    ],
    "cpu": [
        "x64",
        "arm64"
    ],
    "scripts": {
        "format": "bunx prettier --write .",
        "format:check": "bunx prettier --check .",
        "format:rust": "cd lib/rs && cargo fmt --all",
        "format:cpp": "find lib/cpp -name '*.cpp' -o -name '*.h' -o -name '*.cc' | xargs clang-format -i",
        "format:all": "bun run format && bun run format:rust && bun run format:cpp",
        "lint": "bunx eslint .",
        "lint:fix": "bunx eslint . --fix",
        "lint:rust": "cd lib/rs && cargo clippy --all-targets --all-features -- -D warnings",
        "audit": "bun pm ls --all",
        "audit:rust": "cd lib/rs && cargo audit",
        "outdated": "bun outdated",
        "build:cpp": "node-gyp configure build",
        "build:rs": "cd lib/rs && cargo build --release",
        "build:rs:dev": "cd lib/rs && cargo build",
        "build": "bun run build:cpp && bun run build:rs",
        "clean:cpp": "node-gyp clean",
        "clean:rust": "cd lib/rs && cargo clean",
        "clean": "bun run clean:cpp && bun run clean:rust && rm -rf node_modules",
        "postinstall": "bun run build",
        "test:rust": "cd lib/rs && cargo test --release",
        "check": "bun run format:check && bun run lint && bun run lint:rust"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/naruyaizumi/liora.git"
    },
    "bugs": {
        "url": "https://github.com/naruyaizumi/liora/issues"
    },
    "homepage": "https://github.com/naruyaizumi/liora#readme",
    "author": {
        "name": "Naruya Izumi",
        "email": "sexystyle088@gmail.com",
        "url": "https://linkbio.co/naruyaizumi"
    },
    "contributors": [
        {
            "name": "Naruya Izumi",
            "url": "https://github.com/naruyaizumi"
        }
    ],
    "funding": {
        "type": "individual",
        "url": "https://github.com/sponsors/naruyaizumi"
    },
    "dependencies": {
        "async-mutex": "^0.5.0",
        "baileys": "github:naruyaizumi/baileys#main",
        "canvas": "^3.2.0",
        "chokidar": "^5.0.0",
        "eventemitter3": "^5.0.1",
        "file-type": "^21.0.0",
        "p-queue": "^9.0.1",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.2",
        "sharp": "^0.34.4",
        "yt-search": "^2.13.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "eslint": "^9.39.1",
        "globals": "^16.5.0",
        "node-addon-api": "^8.5.0",
        "node-gyp": "^12.0.0",
        "prettier": "^3.6.2"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.8",
        "utf-8-validate": "^6.0.4"
    },
    "imports": {
        "#tiktok": "./lib/api/tiktok.js",
        "#instagram": "./lib/api/instagram.js",
        "#twitter": "./lib/api/twitter.js",
        "#threads": "./lib/api/threads.js",
        "#play": "./lib/api/play.js",
        "#spotify": "./lib/api/spotify.js",
        "#ytmp3": "./lib/api/ytmp3.js",
        "#ytmp4": "./lib/api/ytmp4.js",
        "#spotifydl": "./lib/api/spotifydl.js",
        "#removebg": "./lib/api/removebg.js",
        "#remini": "./lib/api/remini.js",
        "#config": "./src/config.js",
        "#global": "./src/global.js",
        "#message": "./lib/core/message.js",
        "#socket": "./lib/core/socket.js",
        "#connection": "./lib/core/connection.js",
        "#add-on": "./lib/cpp/bridge-async.js",
        "#auth": "./lib/auth/auth.js"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "arrowParens": "always",
        "endOfLine": "lf",
        "proseWrap": "preserve",
        "htmlWhitespaceSensitivity": "css",
        "embeddedLanguageFormatting": "auto"
    },
    "trustedDependencies": [
        "baileys",
        "canvas",
        "protobufjs",
        "sharp",
        "bufferutil"
    ]
}