{
    "name": "liora",
    "version": "9.1.3",
    "type": "module",
    "license": "Apache-2.0",
    "scripts": {
        "start": "bun run --smol src/main.js"
    },
    "dependencies": {
        "@msgpack/msgpack": "^3.1.3",
        "async-mutex": "^0.5.0",
        "baileys": "github:naruyaizumi/baileys",
        "eventemitter3": "^5.0.1",
        "p-queue": "^9.1.0"
    },
    "imports": {
        "#api/*": "./src/lib/api/*",
        "#auth": "./src/lib/core/auth/auth.js",
        "#core/*": "./src/lib/core/*",
        "#lib/*": "./src/lib/*"
    },
    "prettier": {
        "printWidth": 100,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "bracketSameLine": false,
        "arrowParens": "always",
        "endOfLine": "lf",
        "proseWrap": "preserve",
        "htmlWhitespaceSensitivity": "css",
        "embeddedLanguageFormatting": "auto"
    },
    "trustedDependencies": [
        "baileys",
        "protobufjs",
        "sharp"
    ]
}
